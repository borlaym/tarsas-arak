
{
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"linterOptions": {
		"exclude": [
			"docs/**/*",
			"config/**/*.js",
			"node_modules/**/*.ts",
			"coverage/lcov-report/*.js"
		]
	},
	"rules": {
		"ordered-imports": false,
		"no-console": false,
		"object-literal-sort-keys": false,
		"interface-name": false,
		"jsx-no-lambda": false,
		"member-ordering": false
	}
}